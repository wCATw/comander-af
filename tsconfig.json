{
  "$schema": "https://json.schemastore.org/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "lib": ["ESNext"],
    "rootDir": "src",
    "outDir": "dist",
    "sourceMap": true,
    "typeRoots": ["node_modules/@types", "node_modules/discord.js/typings", "node_modules/discord-api-types"],
    "removeComments": true,
    "allowSyntheticDefaultImports": true
  },
  "exclude": ["node_modules", "dist"],
  "include": ["src"],
  "files": ["index.ts"]
}